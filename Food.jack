class Food {

	field int x;
	field int y;

	field int nutrition;

	

	/** Constructs a new Food Object */
	constructor Food new(int xVal, int yVal) {

		let x = xVal;
		let y = yVal;

		let nutrition = 5;

		return this;
	}


	
	// Draw Segment
	method void draw() {
		do Main.drawFoodAt(x,y);
		return;
	}

	method void move(int newX, int newY) {
		let x = newX;
		let y= newY;
		return;
	}



}